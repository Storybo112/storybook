{
  "name": "@storybook/react-dom-shim",
  "version": "8.4.0-beta.0",
  "description": "",
  "keywords": [
    "storybook"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/next/code/lib/react-dom-shim",
  "bugs": {
