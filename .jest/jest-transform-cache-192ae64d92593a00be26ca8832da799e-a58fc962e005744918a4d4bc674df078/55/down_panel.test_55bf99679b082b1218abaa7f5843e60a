'use strict';var _down_panel = require('./down_panel');

describe('manager.ui.containers.down_panel', function () {
  describe('mapper', function () {
    test('should give correct data', function () {
      var state = {
        selectedDownPanel: 'sdp' };


      var selectDownPanel = function selectDownPanel() {return 'selectDownPanel';};
      var panels = {
        test1: {},
        test2: {},
        sdp: {} };

      var getPanels = function getPanels() {return panels;};

      var props = {};
      var env = {
        actions: function actions() {return {
            ui: {
              selectDownPanel: selectDownPanel } };},


        context: function context() {return {
            provider: {
              getPanels: getPanels } };} };




      var result = (0, _down_panel.mapper)(state, props, env);

      expect(result.panels).toEqual(panels);
      expect(result.selectedPanel).toEqual('sdp');
      expect(result.onPanelSelect).toBe(selectDownPanel);
    });
  });
});